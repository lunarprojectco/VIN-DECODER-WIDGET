			<key>NSExtensionAttributes</key>
			<dict>
				<key>NCWidgetDisplayMode</key>
				<array>
					<string>compact</string>
					<string>expanded</string>
				</array>
				<key>NCWidgetSupportedFamilies</key>
				<array>
					<integer>1</integer>
					<integer>2</integer>
				</array>
			</dict>
